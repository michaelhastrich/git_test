/*------------------------------------*\
    VARIABLES
\*------------------------------------*/
/* colours */
$l-blue: rgb(75,201,244);//#4bc9f4;
$d-blue: #286cc1;
/* type */
$font-stack: 'Source Sans Pro', Arial, sans-serif;

/*------------------------------------*\
    MAIN
\*------------------------------------*/

/* global box-sizing */
*,
*:after,
*:before {
  box-sizing:border-box;
  -webkit-box-sizing:border-box;
  -moz-box-sizing:border-box;
  -webkit-font-smoothing:antialiased;
  font-smoothing:antialiased;
  text-rendering:optimizeLegibility;
}
html {
  font-size:67.5%;
}
body {
  background:#fff;
  color:$d-blue;
  font-family: $font-stack;
  font-weight:400;
  font-size: 16px;
  font-size: 1.6rem;
  line-height:1.5;
}
/* clear */
.group:before,
.group:after {
    content:' ';
    display:table;
}
.group:after {
    clear:both;
}
.group {
    *zoom:1;
}
img {
  max-width:100%;
  vertical-align:bottom;
}
a, input[type="submit"] {
  color:$l-blue;
  transition: all 0.2s ease-in;
  -moz-transition: all 0.2s ease-in;
  -webkit-transition: all 0.2s ease-in;
}
a{
  text-decoration:underline;
}
a:hover {
  color:$d-blue;
}
a:hover,
a:active {
  outline:0;
}
input:focus {
  outline:0;
  border:1px solid #04A4CC;
}

/*------------------------------------*\
    STRUCTURE
\*------------------------------------*/
/* grid */
/* wrapper */
.wrap, .headwrap, .footerwrap {
  max-width:1170px;
  width:92.5%;
  margin:0 auto;
  position:relative;
}
.wrap{
   margin-bottom: 160px; 
}
.primary{
  margin-bottom:70px;
}
/* header */
.header {
  position:relative;
  min-height: 130px;
}
/* logo */
.logo {
  height: 130px;
  float:left;
}
.logo-img {
  max-width:300px;
  padding-top:40px;
}
/* content areas */
.home-blogrole{
  width: 48.7179%;
  float:left;
}
/* nav */
.nav {
  float: right;
}
.menu{
  display:none;
}
.main-menu{
  margin:0;
  padding:50px 0 0;
}
.sub-menu{
  position: absolute;
  left:-9999px;
  width: 200px;
  padding:0.5em 10px 20px;
  margin:0;
  background:#fff;
  border-bottom:1px solid rgba(88,98,102,0.1);
  border-left:1px solid rgba(88,98,102,0.1);
  border-right:1px solid rgba(88,98,102,0.1);
  z-index:100;
}
.sub-menu .menu-item{
  margin:0;
  padding: 5px 0;
}
.menu-item{
  position:relative;
  display:inline-block;
  text-transform: uppercase;
  margin-left: 20px;
  font-size: 15px;
  font-size:1.5rem;
}
.menu-item a{
  text-decoration: none;
}
.menu-item a:hover, .menu-item a:focus, .current_page_item a{
  color: $d-blue;
}
.menu-item.has-children > a:after{
    content: "\f078";
    font-family: 'FontAwesome';
    font-size: 12px;
    font-size: 1.2rem;
    margin-left: 5px;
    //color: #943d8a;
}
.menu-item.has-children:hover > .sub-menu{
  left:-10px;
}
/* sidebar */
.about{
  margin-bottom: 60px;
}
.testimonial{
  width: 48%;
  color:#000;
  font-style:italic;
  float:left;
}
.testimonial h3, .personal h3{
  font-style:normal;
  color: $d-blue;
  margin: 1em 0 0;
}
.testimonial span{
  color: #999;
  font-style: normal;
}
.personal{
  width: 48%;
  float: right;
}
.personal img{
  float:left;
  margin: 21px 5px 5px 0;
  border: 1px solid #999;
  padding: 3px;
}
.personal p{
  color:#000;
}
/* footer */
.footer {
  background: $l-blue;
  color: #123765;
}
#footer .footerwrap{
  background: url('../img/mountains.png') no-repeat 83% bottom;
  padding-bottom: 20px;
}
#footer .gform_widget{
  width: 48.7179%;
  float: left;
  margin-right:30px;
}
#footer .gform_widget h3{
  font-weight:normal;
}
#footer .gform_body{
  width: 60%;
}
#footer .gfield_label{
  display:none!important;
}
.gform_wrapper .top_label input.large{
  margin-bottom:10px;
  border: 1px solid #399ada;
  border-radius:6px;
  font-size:16px;
  font-size:1.6rem;
  font-style: italic;
  padding: 5px 8px;
  color:#123765;
}
#field_1_3 .ginput_container{
  position:relative;
}
#field_1_3 .ginput_container:after{
  position:absolute;
  content:"optioneel";
  color: #123765;
  margin: 4px 0 0 10px;
  font-style:italic;
}
.gform_wrapper .gform_footer{
  margin:5px 0 0!important;
  padding:0!important;

}
.gform_wrapper .gform_footer input[type=submit]{
  font-family: $font-stack;
  border: 2px solid $d-blue;
  background: $d-blue;
  color:#fff;
  border-radius: 6px;
  padding: 6px 8px;
}
.gform_wrapper .gform_footer input[type=submit]:hover{
  border: 2px solid #fff;
  background:transparent;
}
.footer .widget_text{
  font-size:15px;
  font-size:1.5rem;
  width: 23%;
  padding-top: 50px;
}
.footer .widget_text h3, .footer .widget_text h4{
  margin:0;
}
.footer .widget_text:nth-child(even){
  margin-right:30px;
}
/*------------------------------------*\
    TYPE
\*------------------------------------*/
.blogheader{
  margin-top:0;
  color:#000;
}
#text-4{
  font-size:15px;
  font-size:1.5rem;
  line-height:1.25;
}
#text-4 h3{
  margin:0;
  font-size:15px;
  font-size:1.5rem;
}
#text-4 p{
  margin-top:0;
}

/*------------------------------------*\
    PAGES
\*------------------------------------*/
/* HOMEPAGE */
.home .intro{
