}
.footer .widget_text:nth-child(even){
  margin-right:30px;
}
/*------------------------------------*\
    TYPE
\*------------------------------------*/
.blogheader{
  margin-top:0;
  color:#000;
}
#text-4{
  font-size:15px;
  font-size:1.5rem;
  line-height:1.25;
}
#text-4 h3{
  margin:0;
  font-size:15px;
  font-size:1.5rem;
}
#text-4 p{
