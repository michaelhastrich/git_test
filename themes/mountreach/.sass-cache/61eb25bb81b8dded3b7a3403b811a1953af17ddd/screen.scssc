}
.gform_wrapper .gform_footer input[type=submit]:hover{
  border: 2px solid #fff;
  background:transparent;
}
.footer .widget_text{
  font-size:15px;
  font-size:1.5rem;
  width: 23%;
  padding-top: 50px;
}
.footer .widget_text h3, .footer .widget_text h4{
